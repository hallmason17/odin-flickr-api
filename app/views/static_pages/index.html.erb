<% if params[:user_id].nil? %>
  <div class="container">
    <div class="row">
      <%= render 'search_form' %>
    <% else %>
      <%= render 'search_form' %>
    </div>
    <div class="container d-flex justify-content-center pt-5">
      <% @photos.each do |photo| %>
        <div class="col">
          <img src="<%=Flickr.url_n(photo) %>"/>
        </div>
      <% end %>
    </div>
  <% end %>
